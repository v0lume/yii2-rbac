{
    "name": "v0lume/yii2-rbac",
    "description":"Database role base access control manager for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","meta","meta tags","seo","behavior","active record"],
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Shmanovsky",
            "email": "heads@xakep.ru"
        }
    ],    
    "support": {
        "issues": "https://github.com/v0lume/yii2-rbac/issues?state=open",
        "source": "https://github.com/v0lume/yii2-rbac"
    },
    "suggest": {
        "v0lume/yii2-meta-tags": "DB based model meta data for SEO"
    },
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "johnitvn/yii2-ajaxcrud": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "v0lume\\yii2\\rbac\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "v0lume\\yii2\\rbac\\Bootstrap"
    }
